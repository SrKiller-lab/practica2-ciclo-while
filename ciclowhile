cont_corr = "TRUENOGOD"

while True:
    for i in range(5):
        cont_use = input("Dame la contrase単a  ")
        if cont_corr == cont_use:
            print("Contrase単a correcta")
            break
        print(f"Contrase単a incorrecta, intentos,{i + 1}")
    else: 
        print("Contrase単a incorrecta")
        print("Has alcanzado el limite de intentos")
    break
